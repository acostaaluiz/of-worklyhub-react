  "dependencies": {
    "@toast-ui/calendar": "^2.1.3",
    "antd": "^6.1.0",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.3",
    "dotenv": "^17.2.3",
    "firebase": "^12.7.0",
    "lucide-react": "^0.561.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.6.0",
    "rxjs": "^7.8.2",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
